    "framer-motion": "6.2.8",
    "graphql": "^16.3.0",
    "i18next": "^21.6.14",
    "js-cookie": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash.isequal": "^4.5.0",
    "next": "^12.1.4",
    "next-cookies": "^2.0.3",
    "next-i18next": "^11.0.0",
    "nprogress": "^0.2.0",
    "react": "18.0.0",
    "react-aria": "^3.14.1",
    "react-dom": "18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-i18next": "^11.16.2",
    "react-icons": "^4.3.1",
    "react-keyed-flatten-children": "^1.3.0",
    "react-query": "^3.34.19",
    "react-remove-scroll": "^2.4.4",
    "styled-system": "^5.1.5",
    "theme-ui": "^0.14.2",
