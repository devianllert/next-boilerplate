    "@radix-ui/react-radio-group": "^0.1.5",
    "@radix-ui/react-scroll-area": "^0.1.4",
    "@radix-ui/react-slot": "^0.1.2",
    "@radix-ui/react-switch": "^0.1.5",
    "@sentry/nextjs": "^6.19.3",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "aria-hidden": "^1.1.3",
    "axios": "^0.26.1",
    "chalk": "^5.0.1",
    "clsx": "^1.1.1",
    "cookies": "^0.8.0",
    "date-fns": "^2.28.0",
